<?xml version="1.0" encoding="UTF-8"?>
<content-unit-test xmlns="http://decima.nist.gov/xml/unit-test">
  <requirement>8</requirement>
  <description>xccdf:Benchmark/xccdf:metadata SHALL contain, at minimum, one of each of the Dublin Core terms dc:creator, dc:publisher, dc:contributor, dc:source.
  The dc:source text content has been removed, this should result in a fail.</description>
  <template xmlns="http://csrc.nist.gov/ns/decima/template/1.0"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:ds="http://scap.nist.gov/schema/scap/source/1.2"
  xmlns:oval-def="http://oval.mitre.org/XMLSchema/oval-definitions-5"
  xmlns:xccdf="http://checklists.nist.gov/xccdf/1.2"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" template="classpath:scap-1.4/templates/configuration-single-data-stream-base.xml">
    <replace xpath="//xccdf:Benchmark/xccdf:metadata/dc:source">
      <dc:source></dc:source>
    </replace>
  </template>
  <assessments>
    <schematron-assessment ruleset="classpath:rules/scap/source-data-stream-1.4.sch" handler-class="gov.nist.secauto.scap.validation.decima.requirements.SCAPSchematronHandler">
      <parameter name="datafiles_directory">classpath:data_feeds</parameter>
    </schematron-assessment>
  </assessments>
  <assertions>
    <assert-derived-requirement requirement-id="SRC-8-1" result="FAIL" />
    <assert-remaining quantifier="ALL" result="PASS" />
  </assertions>
</content-unit-test>
